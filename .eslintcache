hod: {\n        elementType: \"select\",\n        elementConfig: {\n          options: [{ value: \"fastest\", displayValue: \"Fastest\" }],\n        },\n        value: \"\",\n      },\n    },\n    loading: false,\n  };\n  orderHandler = (event) => {\n    event.preventDefault();\n    const { ingredients } = this.props;\n\n    // this.setState({ loading: true });\n    const formData = {};\n    for (let item in this.state.orderForm) {\n      formData[item] = this.state.orderForm[item].value;\n    }\n    const order = {\n      ingredients: this.props.ingredients,\n      price: this.props.price,\n      orderData: formData,\n    };\n    this.props.onOrderBurger(order);\n  };\n  InputChangedHandler = (event, id) => {\n    const form = {\n      ...this.state.orderForm,\n    };\n    const updatedElement = {\n      ...form[id],\n    };\n    updatedElement.value = event.target.value;\n    form[id] = updatedElement;\n    this.setState({ orderForm: form });\n  };\n  render() {\n    const formElementsArray = [];\n\n    for (let key in this.state.orderForm) {\n      formElementsArray.push({\n        id: key,\n        config: this.state.orderForm[key],\n      });\n    }\n    let form = (\n      <form onSubmit={this.orderHandler} className={styles.ContactData}>\n        {formElementsArray.map((formElement, index) => {\n          return (\n            <Input\n              changed={(e) => this.InputChangedHandler(e, formElement.id)}\n              key={index}\n              elementType={formElement.config.elementType}\n              value={formElement.config.value}\n              elementConfig={formElement.config.elementConfig}\n            />\n          );\n        })}\n\n        <Button btnType=\"Success\" clicked={this.orderHandler}>\n          ORDER\n        </Button>\n      </form>\n    );\n    if (this.props.loading) {\n      form = <Spinner />;\n    }\n    return (\n      <div>\n        <h4 className={styles.Message}>Enter your Contact Data</h4>\n        {form}\n      </div>\n    );\n  }\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    ingredients: state.burgerBuilder.ingredients,\n    price: state.burgerBuilder.totalPtice,\n    loading: state.order.loading,\n  };\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    onOrderBurger: (orderData) =>\n      dispatch(orderActions.purchaseBurger(orderData)),\n  };\n};\nexport default connect(mapStateToProps, mapDispatchToProps)(ContactData);\n","/home/arsalan/ReactCompleteGuide/burger-app/src/containers/Orders/Orders.js",[],"/home/arsalan/ReactCompleteGuide/burger-app/src/components/Order/Order.js",[],"/home/arsalan/ReactCompleteGuide/burger-app/src/components/Input/Input.js",[],"/home/arsalan/ReactCompleteGuide/burger-app/src/store/actions/actionTypes.js",[],"/home/arsalan/ReactCompleteGuide/burger-app/src/store/reducers/burgerBuilder.js",[],"/home/arsalan/ReactCompleteGuide/burger-app/src/store/actions/index.js",[],"/home/arsalan/ReactCompleteGuide/burger-app/src/store/actions/burgerBuilder.js",[],"/home/arsalan/ReactCompleteGuide/burger-app/src/store/actions/order.js",[],"/home/arsalan/ReactCompleteGuide/burger-app/src/store/reducers/order.js",[],{"ruleId":"150","severity":1,"message":"151","line":26,"column":7,"nodeType":"152","messageId":"153","endLine":26,"endColumn":13},{"ruleId":"154","replacedBy":"155"},{"ruleId":"156","replacedBy":"157"},{"ruleId":"150","severity":1,"message":"158","line":10,"column":13,"nodeType":"152","messageId":"153","endLine":10,"endColumn":24},{"ruleId":"150","severity":1,"message":"159","line":19,"column":13,"nodeType":"152","messageId":"153","endLine":19,"endColumn":30},{"ruleId":"150","severity":1,"message":"160","line":3,"column":10,"nodeType":"152","messageId":"153","endLine":3,"endColumn":17},{"ruleId":"150","severity":1,"message":"161","line":6,"column":8,"nodeType":"152","messageId":"153","endLine":6,"endColumn":13},{"ruleId":"150","severity":1,"message":"162","line":8,"column":10,"nodeType":"152","messageId":"153","endLine":8,"e